                  {% include 'bss-product-labels-configs' %}
                    
  {% render 'zoorix-cart-items' %}




</head>
             

<body class="template-{{ template | replace: '.', ' ' | truncatewords: 1, '' | handle }}{% if settings.enable_currencies %} currencies-enabled{% endif %}" data-transitions="false">
 {% render 'elevar-body-end' %}
  {% comment %}
    Enable below script if page transitions enabled
  {% endcomment %}
  {% comment %}
  <script type="text/javascript">window.setTimeout(function() { document.body.className += " loaded"; }, 25);</script>
  {% endcomment %}

  <a class="in-page-link visually-hidden skip-link" href="#MainContent">{{ 'general.accessibility.skip_to_content' | t }}</a>

  <div id="PageContainer" class="page-container">
    <div class="transition-body">
    {% if page.handle == 'trio' or  page.handle == 'quad' or page.handle == 'wireless' or page.handle == 'fathers-day' or page.handle == 'wirelessquad' or page.handle == 'black-friday' %}
        {% section 'header-new' %}
     {% else %}
    	{% section 'header' %}
    {% endif %}
      <main class="main-content" id="MainContent">
        {{ content_for_layout }}
        {% include "arp" %}
      </main>
      {% section 'footer-promotions' %}
      {% section 'footer' %}
    





  {% section 'newsletter-popup' %}

  {% if settings.cart_type == 'drawer' %}
    {% include 'ajax-cart-template' %}
  {% endif %}

  {% if settings.enable_currencies %}
    <span
      id="CurrencyData"
      class="hide"
      data-shop-currency="{{ shop.currency }}"
      data-default-currency="{{ settings.default_currency | default: shop.currency }}"
      data-format="{{ settings.currency_format | default: 'money_with_currency_format' }}"
      data-money-format="{{ shop.money_format | strip_html }}"
      data-money-currency-format="{{ shop.money_with_currency_format | strip_html }}">
    </span>
  {% endif %}

  {% include 'qty-selector-template' %}
  {% include 'video-modal' %}
  {% include 'photoswipe-template' %}
  {% if template contains 'product' %}
    {% include 'recently-viewed-product-template' %}
  {% endif %}
  {% render 'upsell-now', customer: customer, product: product, template: template, cart: cart %}
  {% render 'quantity-breaks-now', customer: customer, template: template, product: product, cart: cart %}
      
{% render 'spurit_po2_theme_snippet' %}

  {% render 'preorder-now' %}
  
  <!--   ----animation library---- -->
<script src="https://unpkg.com/aos@2.3.1/dist/aos.js"></script>

  <!-- CRITEO START -->
{% include 'criteo-tracking' %}
<!-- CRITEO END -->


